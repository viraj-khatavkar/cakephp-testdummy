{
    "name": "viraj/cakephp-testdummy",
    "description": "Factories for CakePHP tests",
    "keywords": [
        "testing",
        "cakephp",
        "testdummy",
        "stubs"
    ],
    "require": {
        "php": ">=7.2.0",
        "fakerphp/faker": "~1.4",
        "symfony/finder": "^3.3",
        "cakephp/cakephp": "^4.0",
        "cakephp/chronos": "^2.0",
        "cakephp/migrations": "^3.0"
    },
    "require-dev": {
		"phpunit/phpunit": "^5.7|^6.0|^7.0|^8.5"
    },
    "autoload": {
        "psr-4": {
            "TestDummy\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "conflict": {
        "phpunit/phpunit": "<5.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Viraj Khatavkar",
            "email": "viraj.2438@gmail.com"
        }
    ]
}
